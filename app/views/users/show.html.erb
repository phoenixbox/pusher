<div class="row">
  <div class="span9">Span 9</div>
  <div class="span3">
    <% title "Chat" %>
    <ul id="chat">
      <%= render @messages %>
    </ul>
    <%= form_for Message.new, :remote => true do |f| %>
      <%= f.text_field :content %>
      <%= f.submit "Send" %>
    <% end %>
  </div>
</div>